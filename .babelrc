{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-object-rest-spread"],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-transform-runtime"],
    ["styled-components"],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {}
      }
    ]
  ]
}
